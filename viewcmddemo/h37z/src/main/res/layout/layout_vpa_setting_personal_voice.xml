<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="viewModel"
            type="com.voyah.h37z.VpaViewModel" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="68px"
        android:paddingBottom="68px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/setting_custom_greet">

        <TextView
            android:id="@+id/tv_personal_voice_title"
            style="@style/common_vpa_text_title"
            android:text="@string/settings_personal_voice_title"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerView_personal_voice"
            android:layout_width="1300px"
            android:layout_height="wrap_content"
            android:layout_marginTop="26px"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_personal_voice_title" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>