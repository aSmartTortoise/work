<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="roleName"
            type="String" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cst_item_personal_voice"
        android:layout_width="390px"
        android:layout_height="122px">

        <TextView
            android:id="@+id/tv_item_personal_voice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="46px"
            android:text="@{roleName}"
            android:textColor="#FFFFFF"
            android:textSize="34px"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:list="@{@string/attr_list_value_common}"
            tools:text="温柔女声" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>