<resources>

    <declare-styleable name="SwitchItemView">
        <attr name="labelText" format="string" />
        <attr name="showClickIcon" format="boolean" />
    </declare-styleable>

    <declare-styleable name="VCOSSwitchButton">
        <attr name="check_color" format="color"/>
